Create the Database: Create a database named Global Electronics.
Create the Six Tables: Create tables corresponding to each dataset (Customers, Exchange Rates, Products, Sales, Stores, and Merged) with the necessary schema.

Create the Database
CREATE DATABASE Global_Electronics;
USE Global_Electronics;

Create the Tables
Customers Table
CREATE TABLE Customers (
    customerkey INT PRIMARY KEY,
    gender VARCHAR(50),
    name VARCHAR(255),
    city VARCHAR(100),
    state_code VARCHAR(10),
    state VARCHAR(100),
    zip_code VARCHAR(20),
    country VARCHAR(100),
    continent VARCHAR(50),
    birthday DATE
);

Exchange Rates Table
CREATE TABLE Exchange_Rates (
    id INT AUTO_INCREMENT PRIMARY KEY,
    date DATE,
    currency VARCHAR(10),
    exchange DECIMAL(10, 5)
);

Products Table
CREATE TABLE Products (
    ProductKey INT PRIMARY KEY,
    ProductName VARCHAR(255),
    Brand VARCHAR(100),
    Color VARCHAR(50),
    UnitCostUSD DECIMAL(10, 5),
    UnitPriceUSD DECIMAL(10, 5),
    SubcategoryKey INT,
    Subcategory VARCHAR(100),
    CategoryKey INT,
    Category VARCHAR(100)
);



Sales Table
CREATE TABLE Sales (
    order_number INT PRIMARY KEY,
    line_item INT,
    order_date DATE,
    delivery_date DATE,
    customerkey INT,
    storekey INT,
    productkey INT,
    quantity INT,
    currency_code VARCHAR(10),
    FOREIGN KEY (customerkey) REFERENCES Customers(customerkey),
    FOREIGN KEY (storekey) REFERENCES Stores(storekey),
    FOREIGN KEY (productkey) REFERENCES Products(productkey)
);

Stores Table
CREATE TABLE Stores (
    storekey INT PRIMARY KEY,
    country VARCHAR(100),
    state VARCHAR(100),
    square_meters DECIMAL(10, 2),
    open_date DATE
);

Merged Table
CREATE TABLE Merged (
    OrderNumber INT,
    LineItem INT,
    OrderDate DATE,
    DeliveryDate DATE,
    CustomerKey INT,
    StoreKey INT,
    ProductKey INT,
    Quantity INT,
    CurrencyCode VARCHAR(10),
    CustomerKey_2 INT,
    Gender VARCHAR(10),
    CustomerName VARCHAR(255),
    City VARCHAR(100),
    StateCode VARCHAR(10),
    State VARCHAR(100),
    ZipCode VARCHAR(20),
    Country VARCHAR(100),
    Continent VARCHAR(50),
    Birthday DATE,
    ProductName VARCHAR(255),
    Brand VARCHAR(100),
    Color VARCHAR(50),
    UnitCostUSD DECIMAL(10, 5),
    UnitPriceUSD DECIMAL(10, 5),
    SubcategoryKey INT,
    Subcategory VARCHAR(100),
    CategoryKey INT,
    Category VARCHAR(100),
    StoreCountry VARCHAR(100),
    StoreState VARCHAR(100),
    SquareMeters DECIMAL(10, 2),
    OpenDate DATE,
    Currency VARCHAR(10),
    ExchangeRate DECIMAL(10, 5)
);
